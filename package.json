{
  "name": "my-dreams",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:8000/graphql schema-temp.gql",
    "schema:convert": "node convertToNotNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated ",
    "types:generate": "yarn schema:download && schema:convert && codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "autoprefixer": "^10.4.2",
    "axios": "^0.26.0",
    "graphql": "^15.4.0",
    "next": "12.0.10",
    "postcss": "^8.4.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.27.0",
    "react-icons": "^4.3.1",
    "styled-icons": "^10.45.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.16",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "apollo": "^2.32.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "4.5.5"
  }
}
